  "description": "Beautiful React UI component built with Material-UI to manage (create, update and delete) a simple object list",
  "contributors": [
    {
      "name": "DELACOURT Vincent",
      "email": "vincent.delacourt@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:vdelacou/iblis-simple-crud.git"
  },
  "bugs": {
    "url": "https://github.com/vdelacou/iblis-simple-crud/issues"
  },
  "homepage": "https://github.com/vdelacou/iblis-simple-crud#readme",
  "main": "dist/index.js",
  "module": "dist/iblis-simple-crud.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx lint src test && tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint src test",
    "storybook": "start-storybook -p 9010 --docs",
    "build-storybook": "build-storybook --docs -o ./docs"
  },
  "peerDependencies": {
    "@material-ui/core": ">=4.8",
    "@material-ui/icons": ">=4.8",
    "formik": ">=2",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src test"
    }
  },
  "prettier": {
    "printWidth": 180,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "@storybook/addon-docs": "latest",
    "@storybook/addons": "latest",
    "@storybook/react": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/jest": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "awesome-typescript-loader": "latest",
    "formik": "latest",
    "husky": "latest",
    "react": "latest",
    "react-docgen-typescript-loader": "latest",
    "react-dom": "latest",
    "react-scripts": "latest",
    "tsdx": "latest",
    "typescript": "latest"
  }
}